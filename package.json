{
  "name": "@netgen/admin-ui",
  "version": "1.0.2",
  "description": "NPM package providing base styles for building admin UI interfaces",
  "keywords": [
    "admin",
    "admin ui",
    "style",
    "netgen"
  ],
  "author": "Netgen <info@netgen.hr>",
  "homepage": "https://github.com/netgen/admin-ui-styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netgen/admin-ui-styles.git"
  },
  "scripts": {
    "css": "npm-run-all --parallel css-compile* --sequential css-prefix* css-minify*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/style.scss dist/css/style.css",
    "css-prefix": "postcss --use autoprefixer --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/style.min.css dist/css/style.css",
    "watch": "nodemon --ignore dist/ -e scss -x \"yarn run css\"",
    "js": "webpack"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-css-cli": "^4.1.11",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "uglify-js": "^3.3.28",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "browserslist": [
    "last 2 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "main": "dist/js/bundle",
  "dependencies": {
    "jquery": "^3.3.1",
    "jstree": "^3.3.5"
  }
}
